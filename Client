-- Scripts:

local function NXJJ_fake_script() -- FlingHax.LocalScript 
	local script = Instance.new('LocalScript', FlingHax)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ibrahimtahaxd/Fe-Fling-Hax/main/XD"))()
	end)
end
coroutine.wrap(NXJJ_fake_script)()
local function BEYEKXQ_fake_script() -- RemoveBarriers.Remove 
	local script = Instance.new('Script', RemoveBarriers)

	-- Works For 'AGirlJennifer's Place'
	script.Parent.MouseButton1Click:Connect(function()
	  while true do
	  wait(0.1)
		game.Workspace.KillPart1:Destroy()
	   end
	script.Parent.AlreadyCleared.Disabled = true
	wait(1.5)
	script.Disabled = true
	end) 
end
coroutine.wrap(BEYEKXQ_fake_script)()
local function IVLTY_fake_script() -- RemoveBarriers.AlreadyCleared 
	local script = Instance.new('LocalScript', RemoveBarriers)

	script.Disabled = true
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Text = "Already Cleared..."
		wait(1.5)
		script.Parent.Text = "Remove Barriers"
	end)
end
coroutine.wrap(IVLTY_fake_script)()
local function XNWU_fake_script() -- RemoveBarriers.Remove2 
	local script = Instance.new('Script', RemoveBarriers)

	-- Works For 'AGirlJennifer's Place'
	script.Parent.MouseButton1Click:Connect(function()
	  while true do
	  wait(0.1)
	    game.Workspace.KillPart2:Destroy()
	   end
	end) 
end
coroutine.wrap(XNWU_fake_script)()
local function OMENLO_fake_script() -- Frame.Draggable 
	local script = Instance.new('LocalScript', Frame)

	local frame = script.Parent
	frame.Draggable = true
	frame.Active = true
end
coroutine.wrap(OMENLO_fake_script)()
