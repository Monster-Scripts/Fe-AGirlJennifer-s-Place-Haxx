local function BIAMGBE_fake_script() -- Speed.SetSpeed 
	local script = Instance.new('LocalScript', Speed)

	local SpeedButton = script.Parent
	local Help = script.Parent.Parent.HelpGui
	local SpeedBox = script.Parent.Parent.SpeedBox
	
	
	
	SpeedButton.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = ''..SpeedBox.Text..''
		if SpeedBox.Text == "" then
			SpeedButton.Text = "Enter Number To Start Speed"
		end
		--
		if SpeedBox.Text == "Enter Here Number To Set Speed" then
			SpeedButton = "Enter Number To Start Speed"
		end
	end)
	
	SpeedButton.MouseEnter:Connect(function()
	    Help.Visible = true	
		Help.Text = "This Ä°s Sets Speed"
	end)
	
	SpeedButton.MouseLeave:Connect(function()
		Help.Visible = false
		Help.Text = ""
	end)
end
coroutine.wrap(BIAMGBE_fake_script)()
local function QKUJJJX_fake_script() -- SpeedBox.LocalScript 
	local script = Instance.new('LocalScript', SpeedBox)

	local SpeedBox = script.Parent
	local SetSpeed = script.Parent.Parent.Speed
	local Help = script.Parent.Parent.HelpGui
	
	SpeedBox.MouseEnter:Connect(function()
		Help.Visible = true
	    Help.Text = "Enter Here A Number And Press Set Speed To Set Your Speed."
	end)
	
	SpeedBox.MouseLeave:Connect(function()
		Help.Visible = false
		Help.Text = ""
	end)
end
coroutine.wrap(QKUJJJX_fake_script)()
