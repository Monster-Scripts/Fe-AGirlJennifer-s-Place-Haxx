-- Scripts:

local function PHEYP_fake_script() -- FlingHax.LocalScript 
	local script = Instance.new('LocalScript', FlingHax)

	local Help = script.Parent.Parent.HelpGui
	local RB = script.Parent
	
	RB.MouseEnter:Connect(function()
		Help.Visible = true
		Help.Text = "You Can Fling People"
	end)
	
	RB.MouseLeave:Connect(function()
	   Help.Visible = false
	   Help.Text = ""	
	end)
end
coroutine.wrap(PHEYP_fake_script)()
local function BIBWZN_fake_script() -- FlingHax.LocalScript 
	local script = Instance.new('LocalScript', FlingHax)

	yeet = 'https://raw.githubusercontent.com/ibrahimtahaxd/Fe-Fling-Gui/main/Fe'
	
	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(yeet))()
	end)
end
coroutine.wrap(BIBWZN_fake_script)()
local function WWSKXL_fake_script() -- RemoveBarriers.Remove 
	local script = Instance.new('Script', RemoveBarriers)

	-- Works For 'AGirlJennifer's Place'
	script.Parent.MouseButton1Click:Connect(function()
	  while true do
	  wait(0.1)
		game.Workspace.KillPart1:Destroy()
	   end
	script.Parent.AlreadyCleared.Disabled = true
	wait(1.5)
	script.Disabled = true
	end) 
end
coroutine.wrap(WWSKXL_fake_script)()
local function SBSCTL_fake_script() -- RemoveBarriers.AlreadyCleared 
	local script = Instance.new('LocalScript', RemoveBarriers)

	script.Disabled = true
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Text = "Already Cleared..."
		wait(1.5)
		script.Parent.Text = "Remove Barriers"
	end)
end
coroutine.wrap(SBSCTL_fake_script)()
